<div class="container">
  <%= simple_form_for [@item, @booking] do |f| %>
    <%= f.input :starts_at, required: false, as: :string, input_html: { class: "datepicker" } %>
    <%= f.input :ends_at, required: false, as: :string, input_html: { class: "datepicker" } %>
    <%= f.submit %>
  <% end %>
</div>